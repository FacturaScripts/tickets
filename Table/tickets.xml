<table>
    <column>
        <name>body</name>
        <type>text</type>
    </column>
    <column>
        <name>codagente</name>
        <type>character varying(10)</type>
    </column>
    <column>
        <name>creationdate</name>
        <type>timestamp</type>
    </column>
    <column>
        <name>id</name>
        <type>serial</type>
    </column>
    <column>
        <name>idprinter</name>
        <type>int</type>
    </column>
    <column>
        <name>nick</name>
        <type>character varying(50)</type>
    </column>
    <column>
        <name>printed</name>
        <type>boolean</type>
    </column>
    <column>
        <name>title</name>
        <type>character varying(100)</type>
    </column>
    <constraint>
        <name>tickets_pkey</name>
        <type>PRIMARY KEY (id)</type>
    </constraint>
    <constraint>
        <name>ca_tickets_printers</name>
        <type>FOREIGN KEY (idprinter) REFERENCES tickets_printers (id) ON DELETE CASCADE ON UPDATE CASCADE</type>
    </constraint>
    <constraint>
        <name>ca_tickets_agentes</name>
        <type>FOREIGN KEY (codagente) REFERENCES agentes (codagente) ON DELETE CASCADE ON UPDATE CASCADE</type>
    </constraint>
</table>